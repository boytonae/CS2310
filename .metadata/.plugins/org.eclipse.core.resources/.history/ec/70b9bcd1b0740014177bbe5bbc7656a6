public class Thing
{
	public static void main(String[] args)
	{
		@SuppressWarnings("unused")
		Thing t = new Thing();
	}
	public Thing()
	{
		System.out.println("Hello, there. This is a test.");
	}
	
	public String toCamelCase(String s)
	{
		char[] t = s.toCharArray();
		int i = 0;
		while (i<t.length)
		{
			if(t[i]==" ")
			{
				
			}
		}
	}
	
	
}